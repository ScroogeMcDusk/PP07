<?xml version="1.0" encoding="UTF-8"?><sqlb_project><db path="База данных для системы обнаружения дефектов автомобильных кузовов на конвейере.db" readonly="0" foreign_keys="1" case_sensitive_like="0" temp_store="0" wal_autocheckpoint="1000" synchronous="2"/><attached/><window><main_tabs open="structure browser pragmas query" current="0"/></window><tab_structure><column_width id="0" width="300"/><column_width id="1" width="0"/><column_width id="2" width="100"/><column_width id="3" width="2071"/><column_width id="4" width="0"/><expanded_item id="0" parent="1"/><expanded_item id="1" parent="1"/><expanded_item id="2" parent="1"/><expanded_item id="3" parent="1"/></tab_structure><tab_browse><table title="cameras" custom_title="0" dock_id="1" table="4,7:maincameras"/><dock_state state="000000ff00000000fd00000001000000020000000000000000fc0100000001fb000000160064006f0063006b00420072006f00770073006500310100000000ffffffff0000012e00ffffff000000000000000000000004000000040000000800000008fc00000000"/><default_encoding codec=""/><browse_table_settings/></tab_browse><tab_sql><sql name="SQL 1*">CREATE TABLE operators (
    operator_id INTEGER PRIMARY KEY,
    name TEXT NOT NULL,
    position TEXT,
    cert_number TEXT
);

CREATE TABLE car_bodies (
    car_body_id INTEGER PRIMARY KEY,
    serial_number TEXT NOT NULL UNIQUE,
    model TEXT,
    inspection_date TEXT NOT NULL,
    operator_id INTEGER NOT NULL,
    FOREIGN KEY (operator_id) REFERENCES operators(operator_id)
);

CREATE TABLE defect_types (
    defect_type_id INTEGER PRIMARY KEY,
    name TEXT NOT NULL,
    description TEXT
);

CREATE TABLE defects (
    defect_id INTEGER PRIMARY KEY,
    car_body_id INTEGER NOT NULL,
    defect_type_id INTEGER NOT NULL,
    x_coord INTEGER,
    y_coord INTEGER,
    width_mm REAL,
    height_mm REAL,
    severity TEXT,
    FOREIGN KEY (car_body_id) REFERENCES car_bodies(car_body_id),
    FOREIGN KEY (defect_type_id) REFERENCES defect_types(defect_type_id)
);

CREATE TABLE cameras (
    camera_id INTEGER PRIMARY KEY,
    model TEXT,
    resolution TEXT,
    location TEXT
);

CREATE TABLE images (
    image_id INTEGER PRIMARY KEY,
    car_body_id INTEGER NOT NULL,
    camera_id INTEGER,
    image_path TEXT NOT NULL,
    captured_at TEXT,
    FOREIGN KEY (car_body_id) REFERENCES car_bodies(car_body_id),
    FOREIGN KEY (camera_id) REFERENCES cameras(camera_id)
);
</sql><current_tab id="0"/></tab_sql></sqlb_project>
